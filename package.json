{
  "name": "giancarlobuomprisco.com",
  "version": "1.0.0",
  "description": "Giancarlo Buomprisco's website",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "sitemap": "next-sitemap",
    "start": "next start",
    "typecheck": "tsc",
    "lint": "next lint",
    "export": "next export",
    "static": "next build && next export && npm run sitemap && touch ./out/.nojekyll && echo 'giancarlobuomprisco.com' > ./out/CNAME",
    "deploy": "gh-pages -d out -t true",
    "release": "npm run static && npm run deploy"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^12.0.0",
    "classnames": "2.3.1",
    "date-fns": "2.21.3",
    "gh-pages": "^3.2.3",
    "gray-matter": "4.0.3",
    "next": "latest",
    "next-mdx-remote": "^3.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark": "13.0.0",
    "remark-html": "13.0.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/remark-prism": "^1.3.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.1",
    "next-remote-watch": "^1.0.0",
    "next-sitemap": "^1.6.192",
    "postcss": "^8.3.0",
    "prism-material-themes": "^1.0.4",
    "remark-prism": "^1.3.6",
    "tailwindcss": "^2.1.2"
  }
}
